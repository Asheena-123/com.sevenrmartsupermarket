<suite name="RegressionTesting" thread-count="5" parallel="tests">
	<listeners>
		<listener>
			<class name="com.sevenrmartsupermarket.listeners.ExtentReporter"></class>
		</listener>
	</listeners>
	<test name="RunInChrome">
		<parameter name="browser" value="Chrome"></parameter>
		<classes>
			<class name="com.sevenrmartsupermarket.tests.LoginTest"> </class>
		</classes>
	</test>
	<test name="RunInFirefox">
		<parameter name="browser" value="Firefox"></parameter>
		<classes>
			<class name="com.sevenrmartsupermarket.tests.HomeTest"></class>
		</classes>
	</test>
	<test name="RunInEdge">
		<parameter name="browser" value="Edge"></parameter>
		<classes>
			<class name="com.sevenrmartsupermarket.tests.HomeTest"></class>
		</classes>
	</test>
</suite>